// This is your Courier schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "courier-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Book {
  id Int @id @default(autoincrement())
  createdAt DateTime @default(now())
  title String
  author String
}

model Tarif {
  id Int @id @default(autoincrement())
  createdAt DateTime @default(now())
  origin String
  destination String
  price Int
}
